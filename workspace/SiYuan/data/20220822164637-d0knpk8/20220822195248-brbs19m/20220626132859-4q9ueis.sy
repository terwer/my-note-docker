							"Properties": {
								"id": ""
							}
						},
						{
							"Type": "NodeText",
							"Data": "Bootstrap class loader.",
							"Properties": {
								"id": ""
							}
						},
						{
							"Type": "NodeStrongA6kCloseMarker",
							"Data": "**",
							"Properties": {

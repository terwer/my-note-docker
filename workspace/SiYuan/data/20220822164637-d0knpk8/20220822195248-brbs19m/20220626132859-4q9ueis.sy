							}
						},
						{
							"Type": "NodeText",
							"Data": "Platform class loader.",
							"Properties": {
								"id": ""
							}
						},
						{
							"Type": "NodeStrongA6kCloseMarker",
							"Data": "**",
							"Properties": {
